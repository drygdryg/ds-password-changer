msgid "Error — please fill in the required form fields"
msgstr "Помилка — заповніть необхідні поля форми"

msgid "Error: password confirmation does not match the password"
msgstr "Помилка: підтвердження пароля не відповідає паролю"

msgid "Attempting to change password of the protected user {username}"
msgstr "Спроба змінити пароль захищеного користувача {username}"

msgid "Error: the specified user is protected from changes"
msgstr "Помилка: вказаний користувач захищений від змін"

msgid "Unsuccessful attempt to change password for user {username}: {e}"
msgstr "Невдала спроба змінити пароль для користувача {username}: {e}"

msgid "Error: the user with the specified name was not found"
msgstr "Помилка: користувач із зазначеним ім'ям не знайден"

msgid "Error: the user's credentials were revoked — make sure that the user is not blocked in the domain"
msgstr "Помилка: облікові дані користувача були відкликані - переконайтеся, що користувач не заблокований у домені"

msgid "Authentication failed: make sure old password is correct"
msgstr "Помилка аутентифікації: переконайтеся, що старий пароль вірний"

msgid "Password change rejected. This could have happened due to its too frequent change or its inconsistency with domain security policies"
msgstr "Зміна пароля відхилена. Це могло статися через занадто часту зміну або невідповідність політиці безпеки домену"

msgid "Unknown error: {err_msg} ({err_code})"
msgstr "Невідома помилка: {err_msg} ({err_code})"

msgid "Password changed successfully for user {username}"
msgstr "Пароль успішно змінено для користувача {username}"

msgid "The password was successfully changed"
msgstr "Пароль було успішно змінено"

msgid "The password was not changed for an unknown reason for the user {username}"
msgstr "Пароль не було змінено з невідомої причини для користувача {username}"

msgid "Error: the password was not changed for an unknown reason — contact your administrator"
msgstr "Помилка: пароль не було змінено з невідомої причини - зверніться до свого адміністратора"

msgid "Username"
msgstr "Ім'я користувача"

msgid "Old password"
msgstr "Останній пароль"

msgid "New password"
msgstr "Новий пароль"

msgid "Confirm new password"
msgstr "Новий пароль ще раз"

msgid "Update password"
msgstr "Змінити пароль"
