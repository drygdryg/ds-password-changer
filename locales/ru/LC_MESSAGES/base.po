msgid "Error — please fill in the required form fields"
msgstr "Ошибка — заполните требуемые поля формы"

msgid "Error: password confirmation does not match the password"
msgstr "Ошибка: подтверждение пароля не совпадает с паролем"

msgid "Attempting to change password of the protected user {username}"
msgstr "Попытка изменить пароль защищённого пользователя {username}"

msgid "Error: the specified user is protected from changes"
msgstr "Ошибка: указанный пользователь защищён от изменений"

msgid "Unsuccessful attempt to change password for user {username}: {e}"
msgstr "Неудачная попытка изменить пароль для пользователя {username}: {e}"

msgid "Error: the user with the specified name was not found"
msgstr "Ошибка: пользователь с указанным именем не найден"

msgid "Error: the user's credentials were revoked — make sure that the user is not blocked in the domain"
msgstr "Ошибка: учётные данные пользователя были отозваны — убедитесь, что пользователь не заблокирован в домене"

msgid "Authentication failed: make sure old password is correct"
msgstr "Ошибка аутентификации: убедитесь, что старый пароль введён верно"

msgid "Password change rejected. This could have happened due to its too frequent change or its inconsistency with domain security policies"
msgstr "Изменение пароля отклонено. Это могло произойти по причине слишком частой его смены либо его несоответствия политикам безопасности домена"

msgid "Unknown error: {err_msg} ({err_code})"
msgstr "Неизвестная ошибка: {err_msg} ({err_code})"

msgid "Password changed successfully for user {username}"
msgstr "Пароль успешно изменён для пользователя {username}"

msgid "The password was successfully changed"
msgstr "Пароль успешно изменён"

msgid "The password was not changed for an unknown reason for the user {username}"
msgstr "Пароль не был изменён по неизвестной причине для пользователя {username}"

msgid "Error: the password was not changed for an unknown reason — contact your administrator"
msgstr "Ошибка: пароль не был изменён по неизвестной причине — обратитесь к администратору"

msgid "Username"
msgstr "Имя пользователя"

msgid "Old password"
msgstr "Старый пароль"

msgid "New password"
msgstr "Новый пароль"

msgid "Confirm new password"
msgstr "Новый пароль ещё раз"

msgid "Update password"
msgstr "Изменить пароль"
